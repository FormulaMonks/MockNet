<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RootNamespace>Theorem.MockNet</RootNamespace>
    <Title>System.Net mocking library for HttpClient</Title>
    <Version>1.0.1</Version>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>Devin Sackett</Authors>
    <PackageProjectUrl>https://github.com/citrusbyte/MockNet</PackageProjectUrl>
    <PackageIcon>logo.png</PackageIcon>
    <PackageTags>system.net system.net.http httpclient mocking mock library moq mockhttp test fake stub httpmock</PackageTags>
    <Copyright>Copyright Â©2020</Copyright>
    <Description>A mocking framework to mock HttpClient requests.</Description>
    <NeutralLanguage>en</NeutralLanguage>
    <RepositoryUrl>https://github.com/citrusbyte/MockNet</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
    <PackageReference Include="System.Memory" Version="4.5.3" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\assets\logo.png" Pack="true" Visible="false" PackagePath="\" />
  </ItemGroup>

</Project>
